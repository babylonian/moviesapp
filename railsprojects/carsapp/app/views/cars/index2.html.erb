
<table>
  <tr>
    <th>Reg</th>
    <th>Make</th>
    <th>Model</th>
    <th>Engine</th>
    <th>Size</th>
    <th>Price</th>
    <th>Photo</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.reg %></td>
    <td><%= car.make %></td>
    <td><%= car.model %></td>
    <td><%= car.engine %></td>
    <td><%= car.size %></td>
    <td><%= car.price %></td>
    <td><%= car.photo %></td>
    <td><%= car.description %></td>
    <td><%= link_to 'Show', car %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
    <td><%= link_to 'Destroy', car, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>